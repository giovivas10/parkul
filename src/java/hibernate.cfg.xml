<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/parkul?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.show_sql">true</property>
    <property name="hibernate.current_session_context_class">thread</property>
    <property name="hibernate.hbm2ddl.auto">validate</property>
    <mapping resource="pojo/Objetos.hbm.xml"/>
    <mapping resource="pojo/Usuario.hbm.xml"/>
    <mapping resource="pojo/Propietario.hbm.xml"/>
    <mapping resource="pojo/ParteVehiculo.hbm.xml"/>
    <mapping resource="pojo/DaniosVehiculo.hbm.xml"/>
    <mapping resource="pojo/Rol.hbm.xml"/>
    <mapping resource="pojo/ObjetosVehiculos.hbm.xml"/>
    <mapping resource="pojo/TipoVehiculo.hbm.xml"/>
    <mapping resource="pojo/ParametroEvaluacion.hbm.xml"/>
    <mapping resource="pojo/EvaluacionEstadoVehicular.hbm.xml"/>
    <mapping resource="pojo/TipoUsuario.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
